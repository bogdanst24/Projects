<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ass1.da.CityEntity" table="city">
		<meta attribute="class-description">
			This class contains the user detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="name" not-null="true" column="name" type="string" />
		<property name="lat" not-null="true" column="lat" type="string" />
		<property name="lon" not-null="true" column="lon" type="string" />
	</class>
</hibernate-mapping>