<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ass1.da.SessionEntity" table="session">
		<meta attribute="class-description">
			This class contains the user detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="token" not-null="true" column="token" type="string" />
		<property name="dateCreated" not-null="true" column="dateCreated" type="string" />
		<property name="username" not-null="true" column="username" type="string" />
	</class>
</hibernate-mapping>